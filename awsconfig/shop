server {
    listen 80;
    server_name vepromec.com www.vepromec.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/ubuntu/test-maker-b;
    }

    location /media/ {
        root /home/ubuntu/test-maker-b;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }
}